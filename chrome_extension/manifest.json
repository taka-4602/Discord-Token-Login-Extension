{
   "manifest_version": 3,
   "name": "Discord Token Login - Ozeu Chan Edition",
   "version": "1.0",
   "description": "Log in to Discord using Tokens, and save or manage them easily",
   "action": {
      "default_icon": "icon_128.png",
      "default_popup": "index.html"
   },
   "icons": {
      "16": "icon_16.png",
      "48": "icon_48.png",
      "128": "icon_128.png"
   },   "permissions": [
      "storage"
   ],
   "web_accessible_resources": [
      {
         "resources": ["tab.html"],
         "matches": ["<all_urls>"]
      }
   ],
   "host_permissions": [
      "https://discord.com/api/*"
   ],
   "content_scripts": [
      {
         "js": [ "content.js" ],
         "matches": [ "https://*.discord.com/*", "http://*.discord.com/*" ],
         "run_at": "document_start"
      }
   ]
}